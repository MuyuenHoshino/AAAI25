{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "SGGN",
    "dataset": "MNIST",
    
    "out_dir": "out/superpixels_graph_classification/",
    
    "params": {
        "seed": 35,
        "epochs": 100,
        "batch_size": 128,
        "init_lr": 0.0007,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 15,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 56,
        "d_Conv": 4,
        "hp_Expand": 2,
        "max_degree": 18,
        "linear_agg": true,

        "edge_feat": true,
        "in_feat_dropout": 0.0,

        "pos_enc": false,
        "pos_enc_dim": 8
    }
}