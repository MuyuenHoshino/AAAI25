{
    "gpu": {
        "use": true,
        "id": 1
    },
    
    "model": "SGGN",
    "dataset": "random",
    "num_nodes": 100,
    "out_dir": "out/random/",
    
    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 2,
        "init_lr": 0.0007,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 15,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    
    "net_params": {
        "L": 10,
        "hidden_dim": 82,
        "d_Conv": 4,
        "hp_Expand": 2,
        "max_degree": 5,
        "linear_agg": true,

        "edge_feat": true,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "self_loop": false,
        "lap_pos_enc": true,
        "pos_enc_dim": 8,
        "wl_pos_enc": false,
        "full_graph": false

        
    }
}
