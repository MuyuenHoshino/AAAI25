{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "Mamba",
    "dataset": "SBM_PATTERN",
    
    "out_dir": "out/PATTERN/",
    
    "params": {
        "seed": 35,
        "epochs": 200,
        "batch_size": 16,
        "init_lr": 1e-3,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 24
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 62,
        "d_Conv": 4,
        "hp_Expand": 2,
        "max_degree": 110,
        "linear_agg": false,

        "in_feat_dropout": 0.0,
        "self_loop": false,
        "lap_pos_enc": true,
        "pos_enc_dim": 2,
        "wl_pos_enc": false,
        "full_graph": false

    }
}
